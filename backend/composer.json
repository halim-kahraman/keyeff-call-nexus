
{
    "name": "keyeff/call-panel-backend",
    "description": "KeyEff Call Panel Backend with PHPMailer integration",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "phpmailer/phpmailer": "^6.9",
        "firebase/php-jwt": "^6.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KeyEff\\CallPanel\\": "src/",
            "KeyEff\\CallPanel\\Models\\": "models/",
            "KeyEff\\CallPanel\\Utils\\": "utils/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KeyEff\\CallPanel\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "prefer-stable": true,
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
